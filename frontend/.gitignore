# dotenv environment variables file
.env
.env*.local

# Vite build output
dist
dist-ssr

# SvelteKit build output
.svelte-kit

# Next.js build output
.next

# Nuxt.js build output
.nuxt

# Remix build output
.cache/
build/
public/build/

# Docusaurus build output
.docusaurus

# Gatsby build output
.cache/
public

# Parcel build output
.parcel-cache

# Storybook build output
storybook-static

# SvelteKit serverless functions
.vercel_build_output

# Temporary files
.tmp
tmp

# macOS
.DS_Store